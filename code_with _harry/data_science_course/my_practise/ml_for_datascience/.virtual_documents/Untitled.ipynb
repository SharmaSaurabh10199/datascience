from sklearn.tree import DecisionTreeClassifier


# features for apple and the orange 
features = [
    [150, 0],  # Apple
    [170, 0],  # Apple
    [130, 1],  # Orange
    [120, 1],  # Orange
]
labels = ["apple", "apple", "orange", "orange"]


clf= DecisionTreeClassifier()


model= clf.fit(features,labels)


model.predict([[120,1]])



